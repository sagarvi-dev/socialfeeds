<div class="container">
    <div class="row">
        <div class="col-lg-12">
             <h1 class="page-header">DashBoard</h1>
        </div>
        <% @users.each do |user| %>
         <%unless user == current_user%>
        <div class="col-lg-3 col-md-4 col-xs-6 ">
            <%=image_tag user.avatar.url, :class => 'avatar img-rounded' %>
             <%=link_to user.email, user%>
        <div class="clear"></div>
      </div>
         <% end %>
         <% end %>
    </div>
</div>


  