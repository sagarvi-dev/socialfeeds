<div class="container">

        <div class="row">

            <div class="col-lg-12">
                <h1 class="page-header">DashBoard</h1>
            </div>
<% @identities.each do |identity| %>
            <div class="col-lg-3 col-md-4 col-xs-6 thumb">
                <a class="thumbnail" href="#">
                   <%= image_tag identity.avatar_url , :class => 'img-responsive'%>
                </a>
                <%=link_to identity.email, identity.user%>
      <div class="clear"></div>
    </div>
<% end %>
</div>


  